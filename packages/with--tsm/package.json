{
  "name": "with--tsm",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prestart": "tsc",
    "start": "node dist/index.js",
    "watch": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'tsm' ./src/index.ts",
    "build": "tsc"
  },
  "dependencies": {
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.22",
    "tsm": "^2.3.0"
  }
}
